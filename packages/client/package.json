{
    "name": "@nyoomy/client",
    "version": "0.1.0",
    "description": "A programmatic interface with which to interact with the Nyoomy API.",
    "main": "lib/index.js",
    "module": "src/index.ts",
    "typings": "lib/index.d.ts",
    "scripts": {
        "start": "yarn run watch",
        "watch": "cross-env NODE_ENV=development webpack --config webpack.config.ts",

        "test": "jest --detectOpenHandles",
        "generate": "graphql-codegen --config codegen.yml",
        "lint": "eslint --cache --fix --color .",
        "format": "prettier -lw --config \".prettierrc\" --ignore-path \".prettierignore\" src",

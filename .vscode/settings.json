{
    "cSpell.words": [
        "GraphQLID",
        "Nyoomy",
        "Prettier's",
        "browserslist",
        "cockroachdb",
        "codegen",
        "estree",
        "fontawesome",
        "fortawesome",
        "graphiql",
        "inmemory",
        "mariadb",
        "nonwords",
        "nyoomyapp",
        "oper",
        "overnightjs",
        "postgres",
        "relogin",
        "sqljs",
        "testid",
        "typeorm"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "json"
    ],
    "editor.rulers": [80, 100],
    "editor.formatOnSave": true,
    "eslint.alwaysShowStatus": true,
    "[git-commit]": {
        "editor.rulers": [50]
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "explorerExclude.backup": {}
}
